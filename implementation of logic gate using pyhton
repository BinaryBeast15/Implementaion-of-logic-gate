l1=[1,0,1,0,1,0,1]
l2=[0,1,0,1,0,1,0]
l3=[]
l4=[]
i=0
k=7
print("l1 = " ,l1)
print("Implementation of NOT gate")
for i in range(k)  :
    if(l1[i] == 0):
       l3.append(1)
    else :
        l3.append(0)
print("l3 =" ,l3)  
print("Implementatoion of OR gate")
print("l1 = ",l1)
print("l2 = ",l2)
for i in range(k)  :
    if((l1[i]+ l2[i]) == 0):
       l4.append(0)
    elif((l1[i]+l2[i]) == 1) :
        l4.append(1)
print("l4 = ",l4)  
